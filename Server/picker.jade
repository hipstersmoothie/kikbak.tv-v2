html
	body
		div
			iframe(src=urlNoRss style="width:100%; height: 90vh")
		div(style="text-align:center")
			a.url(href=url target="_blank") #{url}
			p(id=id)
			button.deleteButton Delete
			button.verifyButton Verify
			button.tumblrButton Tumblr
			p Database Entries
			ul
			each dup in dups
				li #{dup.url}
			p edit url
			div
				input.newUrl
				button.changeUrl change


		script(src='/javascripts/jquery.js', type="text/javascript")
		script(src='/javascripts/main.js', type="text/javascript")